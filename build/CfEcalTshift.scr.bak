# Fonction pour attendre si trop de jobs sont lanc√©s
wait_for_jobs() {
    while [ "$(jobs -r | wc -l)" -ge "$MAX_JOBS" ]; do
        sleep 0.5
    done
}

# Boucle d‚Äôex√©cution
for runlist in "${runlists[@]}"; do
    wait_for_jobs
    echo "Lancement de ./myAnalyse ${runlist}..."
    ./myAnalyse "$runlist" &
done

# Attendre que tous les jobs restants finissent
wait

echo "‚úÖ Tous les traitements sont termin√©s."

